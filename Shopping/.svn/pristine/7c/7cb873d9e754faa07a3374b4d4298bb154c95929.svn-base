  <select id="selectOrder" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from yd_base
    where yd_standy3 = #{id,jdbcType=INTEGER}  and yd_standy7!="1"
    <if test="standby1 != 'admin' and standby1 != ''">
    	and yd_standy1= #{standby1}
    </if>
  </select>
  
  <!-- selectYd查找默认 -->
