  <!--  条件查询所有订单 -->
	<select id="selStatusOrder" parameterType="com.sybinal.shop.model.FLogistics" resultMap="BaseResultMap">
		select 
	    <include refid="Base_Column_List" />
	    from logistics 
	    <where>
	     <if test="let.size()>0">
		    and 
		    <foreach collection="let" item="id" open="(" close=")" separator="or">
			    <if test="id != null and id != ''">
		         f_ids like concat('%','${id}','%')
		      	</if>
			    <if test="id != null and id != ''">
		        or f_client_order_code like concat('%','${id}','%')
		      	</if>
			    <if test="id != null and id != ''">
		        or standby_11 like concat('%','${id}','%')
		      	</if>
			    <if test="id != null and id != ''">
		        or id like concat('%','${id}','%')
		      	</if>
			    <if test="id != null and id != ''">
		        or standby_14 like concat('%','${id}','%')
		      	</if>
	      	</foreach>
	      </if>
	    <if test="flog.standby6 != null and flog.standby6 != ''">
        and standby_6 = #{flog.standby6,jdbcType=VARCHAR}
      	</if> 
	    <if test="flog.startDatas != null and flog.startDatas != ''">
        and date_format(update_time,'%Y-%m-%d') >= #{flog.startDatas,jdbcType=VARCHAR}
      	</if>
	    <if test="flog.endDatas != null and flog.endDatas != ''">
        and  #{flog.endDatas,jdbcType=VARCHAR} >= date_format(update_time,'%Y-%m-%d')
      	</if> 
      	<if test="flog.standby9 == 1">
      	and  (f_ids in (select x.x_f_ids from x_logistics x,logistics a where a.f_ids = x.x_f_ids and x.x_standby1=1)
      	</if>
      	<if test="flog.standby9 == 1">
      	or  f_ids in (select x.hj_shipperHawbcode from hj_base x,logistics a where a.f_ids = x.hj_shipperHawbcode and x.hj_standy2=1))
      	</if>
      	<if test="flog.standby13 == 1">
      	and  standby_13 = "1"
      	</if>
      	<if test="flog.standby13 == 0">
      	and  standby_13 != "1"
      	</if>
      	
      	<if test="flog.standby13 == 2">
      	
      	</if>
      	<if test="flog.standby12 != ''">
      	and  standby_12 =  #{flog.standby12,jdbcType=VARCHAR}
      	</if>
      	</where> 
      	order by f_pay_time    desc
	</select>
